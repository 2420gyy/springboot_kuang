<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chen.mybatispluswrapper.mapper.IStudentMapper">

    <select id="QueryByPage"  resultType="com.chen.mybatispluswrapper.entity.Student">
        select * from Student where id > #{num}
    </select>

    <select id="testForEach"  resultType="com.chen.mybatispluswrapper.entity.Student">
        select * from student where tid in
            <foreach collection="list" item="item" index="index" open="(" separator="," close=")">
                #{item}
            </foreach>
    </select>

</mapper>
